{
  "name": "the-guild-website",
  "version": 2,
  "alias": ["guild"],
  "public": false,
  "github": {
    "silent": true
  },
  "routes": [
    { "src": "api/graphql" },
    {
      "src": "/inspector",
      "status": 301,
      "headers": {"Location": "https://graphql-inspector.com"}
    },
    {
      "src": "/codegen",
      "status": 301,
      "headers": {"Location": "https://graphql-code-generator.com"}
    },
    {
      "src": "/modules",
      "status": 301,
      "headers": {"Location": "https://graphql-modules.com"}
    }
  ],
  "env": {
    "SLACK_TOKEN": "@slack-token"
  }
}
